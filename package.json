    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-source-filesystem": "^2.3.37",
    "gatsby-source-graphql": "^2.7.6",
    "gatsby-transformer-remark": "^2.8.47",
    "node-sass": "^4.14.1",
    "prismjs": "^1.22.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.1.0",
    "scriptjs": "^2.5.9"
